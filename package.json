{
  "name": "innsane",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "obfuscate": "javascript-obfuscator index.js --output obf.js --identifier-names-generator hexadecimal --reserved-strings require,chalk,readline,express,axios",
    "buildObf": "npm run obfuscate && pkg obf.js --targets node18-win-x64 --output dist/mtnkapi-v2-obf.exe",
    "build": "pkg index.js --targets node18-win-x64 --output dist/mtnkapi-v2.exe"
  },
  "bin": {
    "myserver": "index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.5",
    "chalk": "^4.1.2",
    "express": "^4.21.2",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "javascript-obfuscator": "^4.0.3",
    "pkg": "^5.8.1"
  },
  "pkg": {
    "scripts": [
      "obf.js",
      "node_modules/express/index.js",
      "node_modules/axios/index.js",
      "node_modules/readline/lib/readline.js"
    ],
    "assets": [],
    "targets": [
      "node18-win-x64"
    ]
  }
}
